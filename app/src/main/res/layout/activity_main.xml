<?xml version="1.0" encoding="utf-8"?><androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/cl_parent"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/ultra_light_grey"    tools:activity=".activities.MainActivity">    <com.google.android.material.appbar.AppBarLayout        android:id="@+id/appBarLayout"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/ultra_light_grey"        app:elevation="0dp">        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@color/transparent"            app:layout_scrollFlags="scroll|snap|enterAlways">            <androidx.appcompat.widget.Toolbar                android:id="@+id/tl_top_bar"                android:layout_width="match_parent"                android:layout_height="?attr/actionBarSize"                android:animateLayoutChanges="true"                android:background="@color/ultra_light_grey"                android:contentInsetStart="0dp"                android:contentInsetLeft="0dp"                app:contentInsetLeft="0dp"                app:contentInsetStart="0dp">                <RelativeLayout                    android:id="@+id/rl_top_bar_container"                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_margin="@dimen/padding_small"                    android:background="@drawable/action_bar_rounded_bg"                    app:elevation="2dp">                    <ImageView                        android:id="@+id/iv_profile_info"                        android:layout_width="36dp"                        android:layout_height="36dp"                        android:layout_centerVertical="true"                        android:layout_marginLeft="@dimen/padding_medium_small"                        android:background="#00ffffff"                        android:scaleType="fitCenter"                        android:src="@drawable/blank_profile_male"                        android:textColor="@color/white"                        android:textSize="@dimen/text_xl" />                    <TextView                        android:id="@+id/tv_actionbar_title"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:layout_marginLeft="20dp"                        android:layout_toLeftOf="@id/ll_search_filter_wrapper"                        android:layout_toRightOf="@id/iv_profile_info"                        android:text="title"                        android:textColor="@color/white"                        android:textSize="@dimen/text_xxxl"                        android:textStyle="bold"                        />                    <LinearLayout                        android:id="@+id/ll_search_filter_wrapper"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentRight="true"                        android:layout_centerVertical="true"                        android:orientation="horizontal">                        <ImageButton                            android:id="@+id/ib_search"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_centerVertical="true"                            android:background="@color/transparent"                            android:padding="@dimen/padding_medium"                            android:src="@drawable/ic_search_white" />                        <ImageButton                            android:id="@+id/ib_filter"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_marginRight="@dimen/padding_medium"                            android:background="@color/transparent"                            android:padding="@dimen/padding_medium"                            android:src="@drawable/ic_filter_white" />                    </LinearLayout>                    <RelativeLayout                        android:id="@+id/rl_search_wrapper"                        android:layout_width="match_parent"                        android:layout_height="match_parent"                        android:visibility="gone">                        <EditText                            android:id="@+id/et_search_bar"                            android:layout_width="match_parent"                            android:layout_height="?attr/actionBarSize"                            android:layout_centerVertical="true"                            android:background="@color/white"                            android:gravity="left|center_vertical"                            android:hint="@string/search"                            android:imeOptions="actionSearch"                            android:inputType="text"                            android:maxLines="1"                            android:paddingLeft="70dp"                            android:textColor="@color/med_grey"                            android:textColorHint="@color/light_grey"                            android:textSize="@dimen/text_xl" />                        <ImageButton                            android:id="@+id/ib_back_gray"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentLeft="true"                            android:layout_centerVertical="true"                            android:layout_marginLeft="@dimen/padding_large"                            android:background="#00ffffff"                            android:padding="@dimen/padding_medium"                            android:src="@drawable/ic_back_gray" />                        <ImageButton                            android:id="@+id/ib_close_gray"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:layout_marginRight="@dimen/padding_large"                            android:background="#00ffffff"                            android:padding="@dimen/padding_medium"                            android:src="@drawable/ic_gray_cross" />                    </RelativeLayout>                </RelativeLayout>            </androidx.appcompat.widget.Toolbar>            <RelativeLayout                android:id="@+id/rl_filter_wrapper"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@+id/tl_top_bar"                android:visibility="gone">                <TextView                    android:id="@+id/tv_filter"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:drawableLeft="@drawable/ic_filter_gray"                    android:gravity="center_vertical"                    android:padding="@dimen/padding_small"                    android:text="@string/filter"                    android:textColor="@color/med_grey"                    android:textSize="@dimen/text_large" />                <TextView                    android:id="@+id/tv_create_session"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentLeft="true"                    android:layout_marginLeft="@dimen/padding_medium"                    android:drawableLeft="@drawable/ic_session_unfilled"                    android:drawablePadding="@dimen/padding_small"                    android:gravity="center_vertical"                    android:padding="@dimen/padding_small"                    android:text="@string/create_session"                    android:textColor="@color/med_grey"                    android:textSize="@dimen/text_large" />            </RelativeLayout>        </RelativeLayout>    </com.google.android.material.appbar.AppBarLayout>    <com.avadna.luneblaze.utils.NonSwipingViewPager        android:id="@+id/vp_pager"        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:layout_behavior="@string/appbar_scrolling_view_behavior" />    <RelativeLayout        android:id="@+id/rl_bottom_bar_wrapper"        android:layout_width="match_parent"        android:layout_height="120dp"        android:layout_gravity="bottom"        android:orientation="vertical"        app:layout_scrollFlags="scroll|enterAlways">        <com.google.android.material.floatingactionbutton.FloatingActionButton            android:id="@+id/fb_create"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentTop="true"            android:layout_alignParentRight="true"            android:layout_marginRight="@dimen/padding_medium"            app:backgroundTint="@color/app_theme_dark" />        <com.google.android.material.bottomnavigation.BottomNavigationView            android:id="@+id/bot_nav_bar"            android:layout_width="match_parent"            android:layout_height="48dp"            android:layout_alignParentBottom="true"            android:layout_margin="@dimen/padding_small"            android:background="@drawable/white_rounded_border_white_fill"            app:elevation="2dp"            android:paddingLeft="@dimen/padding_small"            android:paddingRight="@dimen/padding_small"            app:itemIconTint="@drawable/navigation_bar_item_state"            app:itemTextColor="@drawable/navigation_bar_item_state"            app:menu="@menu/bottom_nav_bar_menu" />    </RelativeLayout></androidx.coordinatorlayout.widget.CoordinatorLayout>